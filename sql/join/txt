#2 없어진 기록찾기

select b.animal_id, b.name from animal_ins as a right join animal_outs as b on a.animal_id = b.animal_id where a.animal_id is null

#2. 있었는데요 없었습니다.

SELECT b.animal_id, b.name
from 
    animal_ins as a, animal_outs as b
where
    a.animal_id = b.animal_id and a.datetime > b.datetime
order by a.datetime

#3 오랜기간 보호한 동물
SELECT 
    a.name, a.datetime
from
    animal_ins as a left join animal_outs as b on a.animal_id = b.animal_id
where
    b.animal_id is null
order by
    a.datetime limit 3